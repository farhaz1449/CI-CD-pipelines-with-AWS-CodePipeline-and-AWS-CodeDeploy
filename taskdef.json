{
    "family": "myrepo-task",
    "executionRoleArn": "arn:aws:iam::0123456789012:role/service-role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "myrepo-container",
        "image": "<IMAGE1_NAME>",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "taskRoleArn": "arn:aws:iam::0123456789012:role/service-role/appTaskRole"
  }
  